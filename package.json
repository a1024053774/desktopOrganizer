{
  "name": "desktop-organizer",
  "version": "1.0.0",
  "description": "在桌面壁纸上创建苹果风格的半透明圆角矩形区域",
  "main": "src/main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder --win portable"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "electron": "^25.0.0",
    "electron-builder": "^23.0.0"
  },
  "dependencies": {
    "electron-store": "^8.1.0"
  },
  "build": {
    "appId": "com.desktop.organizer",
    "productName": "Desktop Organizer",
    "files": [
      "src/**/*",
      "views/**/*",
      "assets/**/*",
      "package.json"
    ],
    "win": {
      "target": [
        "portable"
      ],
      "icon": "assets/icon.ico"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/a1024053774/desktopOrganizer.git"
  },
  "keywords": [],
  "bugs": {
    "url": "https://github.com/a1024053774/desktopOrganizer/issues"
  },
  "homepage": "https://github.com/a1024053774/desktopOrganizer#readme"
}
